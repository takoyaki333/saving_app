<h2>ログイン</h2>
<%= form_with(url: login_path, local: true) do |f| %>
  <div>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "ログイン" %>
  </div>
<% end %>
<%= link_to "新規登録はこちら", signup_path %>